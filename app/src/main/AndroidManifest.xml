    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/DaTeem"
        android:allowBackup="true">
    <activity
            android:name=".MenuActivity"
            android:configChanges="keyboard|keyboardHidden|orientation"
